icons = [
  "tab_close@2x.png",
  "panel_close@2x.png",
  "folder_open@2x.png",
  "folder_closed@2x.png",

  # TODO: Remove this.
  "stanford_bunny.png",
  "dice.png",
]

if (is_mac) {
  bundle_data("icons") {
    sources = icons
    outputs = [ "{{bundle_resources_dir}}/icons/{{source_file_part}}" ]
  }
}

if (is_linux || is_win) {
  copy("icons") {
    sources = icons
    outputs = [ "$root_out_dir/icons/{{source_file_part}}" ]
  }
}
